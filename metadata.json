{
  "name": "systarch-baseline",
  "version": "1.1.0",
  "author": "systarch",
  "summary": "Baseline installation for all systarch systems",
  "tags": [],
  "license": "proprietary",
  "source": "https://github.com/systarch/puppet-baseline",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.25.0"
    },
    {
      "name": "saz-locales",
      "version_requirement": ">= 2.5.1 < 3.0.0"
    },
    {
      "name": "saz-ssh",
      "version_requirement": ">= 2.3.0 < 7.0.0"
    },
    {
      "name": "camptocamp-postfix",
      "version_requirement": ">= 1.9.0 < 2.0.0"
    },
    {
      "name": "systarch-mailhog",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
	    "name":"puppetlabs-ruby_task_helper",
	    "version_requirement":">= 0.4.0"
    }
  ],
  "operatingsystem_support": [
    { "operatingsystem": "Debian", "operatingsystemrelease": ["8", "9", "10"]},
    { "operatingsystem": "Ubuntu", "operatingsystemrelease": ["20.04"]}
  ]
}
